<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>0</minor>
	</specVersion>
	<serviceStateTable>
		<stateVariable>
			<name>Status</name>
			<sendEventsAttribute>yes</sendEventsAttribute>
			<dataType>ui1</dataType>
			<defaultValue>0</defaultValue>
			<shortCode>status</shortCode>
		</stateVariable>
		<stateVariable>
			<name>RulesEnginePanel</name>
			<sendEventsAttribute>yes</sendEventsAttribute>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
			<shortCode>rulesenginepanel</shortCode>
		</stateVariable>
		<!-- Arguments -->
		<stateVariable>
			<name>A_ARG_TYPE_FileName</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_RuleId</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_RuleIdx</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_RuleName</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_Arming</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_Acknowledgement</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_ConditionId</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable>
			<name>A_ARG_TYPE_ConditionStatus</name>
			<sendEventsAttribute>no</sendEventsAttribute>
			<dataType>ui1</dataType>
		</stateVariable>
	</serviceStateTable>
	<actionList>
		<action>
			<name>LoadRules</name>
			<argumentList>
				<argument>
					<name>fileName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_FileName</relatedStateVariable>
				</argument>
				<argument>
					<name>ruleIdx</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_RuleIdx</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>RemoveRule</name>
			<argumentList>
				<argument>
					<name>fileName</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_FileName</relatedStateVariable>
				</argument>
				<argument>
					<name>ruleIdx</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_RuleIdx</relatedStateVariable>
				</argument>
				<argument>
					<name>ruleId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_RuleId</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetRuleArming</name>
			<argumentList>
				<argument>
					<name>ruleId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_RuleId</relatedStateVariable>
				</argument>
				<argument>
					<name>arming</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Arming</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetRuleAcknowledgement</name>
			<argumentList>
				<argument>
					<name>ruleId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_RuleId</relatedStateVariable>
				</argument>
				<argument>
					<name>acknowledgement</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_Acknowledgement</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<!-- Debug -->
		<action>
			<name>SetConditionStatus</name>
			<argumentList>
				<argument>
					<name>conditionId</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_ConditionId</relatedStateVariable>
				</argument>
				<argument>
					<name>status</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_RuleIdx</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
	</actionList>
</scpd>
